{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESnext",
    "lib": [
      "ES2017",
      "dom"
    ],
    "allowJs": true,
    "jsx": "react-native",
    "checkJs": false,
    "declaration": true,
    "isolatedModules": true,
    "outDir": "dist",
    "strict": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts",
    "App.tsx"
, "src/n_views/visioncamera/views/CaptureButton.tsx", "src/n_views/visioncamera/CebraspeVisionCamera.tsx_OLD", "src/n_views/visioncamera/CebraspeVisionCamera_ORIGINAL.tsx_OLD"  ],
  "exclude": [
    ".ci",
    "web-build",
    "node_modules",
    "babel.config.js",
    "jest.config.js",
    "src/index.d.ts",
    "__tests__"
  ]
}
